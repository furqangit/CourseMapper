
<div class="row" ng-controller="ViewCalibrationController">
    <section class="content">
        <div class="row row-centered">
            <div class="col-xs-12 col-lg-9 col-md-9 col-sm-9 col col-centered">
                <div class="box no-border no-round">
                    <div class="box-header with-border">
                        <h3 class="box-title">View Calibration</h3>
                    </div>
                    <div class="box-body form-horizontal horizontal-max">
                        <!--<form class="form-horizontal horizontal-max" name="addAssignmentForm" role="form" ng-submit="saveNewPost(addAssignmentForm.$valid)" novalidate>-->
                        <fieldset>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">Title</label>
                                <div class="col-sm-6 control-label" style="text-align: left;">
                                    <label style="font-weight: normal">{{viewCalibrationObject.title}}</label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-5 control-label">Teacher Name</label>
                                <div class="col-sm-6 control-label" style="text-align: left;">
                                    <label style="font-weight: normal">{{viewCalibrationObject.teacherName}}</label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-5 control-label">Teacher Comments</label>
                                <div class="col-sm-6 control-label" style="text-align: left;">
                                    <label style="font-weight: normal">{{viewCalibrationObject.teacherComments}}</label>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">Calibration Documents</label>
                                <div class="col-sm-6 control-label" style="text-align: left;">
                                    <div class="label label-info" ng-repeat="doc in viewCalibrationObject.displayCalibrationDocumentsList">
                                        <a href="{{doc.link}}" target="_blank" style="color: white" download> &nbsp;
                                            <span>{{doc.name}}</span> &nbsp;
                                            <br>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                    <!--</form>-->
                </div>
            </div>
        </div>
</div>
</section>

<!--</div>-->
</div>