<div class="row">
    <section class="content">
        <div class="row row-centered">
            <div class="col-xs-12 col-lg-9 col-md-9 col-sm-9 col col-centered">
                <div class="box no-border no-round">
                    <div class="box-header with-border">
                        <h3 class="box-title">Admin Feedback</h3>
                    </div>
                    <div class="box-body">
                        <form class="form-horizontal horizontal-max" name="form" role="form" novalidate>
                            <div class="panel panel-default">
                                <div class="panel-heading">Submitted Solution</div>
                                <div class="panel-body">
                                    <fieldset>
                                        <div class="form-group">
                                            <label class="col-sm-5 control-label">Solution Title</label>
                                            <div class="col-sm-6 control-label" style="text-align: left;">
                                                <label style="font-weight: normal">{{solution.title}}</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-5 control-label">Student Name</label>
                                            <div class="col-sm-6 control-label" style="text-align: left;">
                                                <label style="font-weight: normal">{{solution.studentName}}</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-5 control-label">Solution Documents</label>
                                            <div class="col-sm-6 control-label" style="text-align: left;">
                                                <div class="label label-info" ng-repeat="doc in solution.displayDocumentsList">
                                                    <a id="hiddenFile" href="{{doc.link}}" target="_blank" style="color: white" download> &nbsp;
                                                        <span>{{doc.name}}</span> &nbsp;
                                                        <br>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-5 control-label">Solution Comments</label>
                                            <div class="col-sm-6 control-label" style="text-align: left;">
                                                <label style="font-weight: normal">{{solution.studentComments}}</label>

                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>

                            <div class="panel panel-default">
                                <div class="panel-heading">Analyzed Report</div>
                                <div class="panel-body">
                                    <fieldset>
                                        <div class="form-group">

                                            <div class="col-sm-6 control-label">
                                                <div id="userWordCloud">
                                                    <label style="text-align: center; margin-top: -20px; margin-bottom: 5px;" class="col-sm-12 control-label">{{solution.studentName}} Word Cloud</label>
                                                </div>
                                            </div>

                                            <div class="col-sm-6 control-label">
                                                <div id="solutionWordCloud">
                                                    <label style="text-align: center; margin-top: -20px; margin-bottom: 5px;" class="col-sm-12 control-label">Solution Word Cloud</label>
                                                </div>
                                            </div>

                                        </div>
                                    </fieldset>

                                    <fieldset>
                                        <div class="form-group">
                                            <div class="form-group">
                                                <label style="text-align: center; font-size: 18px;" class="col-sm-12">Similarity report of statistical methods</label>
                                            </div>

                                            <div class="form-group">
                                                <div class="form-group">
                                                    <div class="col-sm-7">

                                                        <div class="form-group">
                                                            <div class="col-sm-12">
                                                                <div class="col-sm-6">
                                                                    <label style="text-align: center;" class="col-sm-12">Cosine similarity</label>
                                                                    <div style="max-height: 200px; height: 200px;" id="chartCosine"></div>
                                                                    <div class="" style="text-align: center;">
                                                                        <!-- <label style="font-weight: normal">{{analyzedRepertData_Obj.CosineSimilarity}} %</label> -->
                                                                    </div>
                                                                </div>

                                                                <div class="col-sm-6">
                                                                    <label style="text-align: center;" class="col-sm-12">Sørensen–Dice coefficient</label>
                                                                    <div style="max-height: 200px; height: 200px;" id="chartDice"></div>
                                                                    <div class="" style="text-align: center;">
                                                                        <!-- <label style="font-weight: normal">{{analyzedRepertData_Obj.DiceCoefficientSimilarity}} %</label> -->
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <br>
                                                        <br>
                                                        <br>
                                                        <div class="form-group">
                                                            <div class="col-sm-12">
                                                                <div class="col-sm-6">
                                                                    <label style="text-align: center;" class="col-sm-12">Levenshtein Distance</label>
                                                                    <div style="max-height: 200px; height: 200px;" id="chartLeven"></div>
                                                                    <div class="" style="text-align: center;">
                                                                        <!-- <label style="font-weight: normal">{{analyzedRepertData_Obj.LevenshteinSimilarity}} %</label> -->
                                                                    </div>
                                                                </div>

                                                                <div class="col-sm-6">
                                                                    <label style="text-align: center;" class="col-sm-12">Jaccard similarity coefficient</label>
                                                                    <div style="max-height: 200px; height: 200px;" id="chartJaccard"></div>
                                                                    <div class="" style="text-align: center;">
                                                                        <!-- <label style="font-weight: normal">{{analyzedRepertData_Obj.JaccardSimilarity}} %</label> -->
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>

                                                    <div style="margin-top: 5%;" class="col-sm-5">

                                                        <label class="col-sm-12" style="text-align: center">Mean</label>
                                                        <!-- <i class="tooltip ionicons ion-ios-help" data-toggle="tooltip" title="States if the solution will be reviewed by 1 or multiple"></i> -->

                                                        <div style="margin-right: 48px; text-align: right;" class="tooltip1">
                                                            <img style="width: 26px" src="/img/tooltip.png">
                                                            <span class="tooltiptext">Above 60% is considered a good score</span>
                                                        </div>

                                                        <div style="max-height: 300px; height: 300px;" id="chartMean"></div>
                                                        <div class="" style="text-align: center;">
                                                            <!-- <label style="font-weight: normal">{{analyzedRepertData_Obj.Mean}} %</label> -->
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                    </fieldset>
                                    </div>
                                </div>

                                <div class="panel panel-default">
                                    <div class="panel-heading">Peer Reviews</div>
                                    <div class="panel-body">
                                        <fieldset>
                                            <div ng-show="!reviews || reviews.length == 0">
                                                <h5>No reviews available currently</h5>
                                            </div>
                                            <table class="table table-hover" ng-show="reviews.length > 0">
                                                <thead>
                                                    <tr>
                                                        <th>Student Name</th>
                                                        <th>Obtained Marks</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="review in reviews" ng-click="openReview(review)">
                                                        <td>{{review.assignedTo.displayName}}</td>
                                                        <td>{{review.marksObtained}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </fieldset>

                                        <div ng-show="reviews.length > 0" class="form-group">
                                            <fieldset>
                                                <div class="col-sm-12">
                                                    <div class="col-sm-6">
                                                        <label style="text-align: center; font-size: 16px;" class="col-sm-12">{{solution.studentName}} obtained marks</label>
                                                        <div id="marksObtainedBarChart"></div>
                                                    </div>

                                                    <div class="col-sm-6">
                                                        <label style="text-align: center; font-size: 16px;" class="col-sm-12">{{solution.studentName}} obtained marks mean</label>
                                                        <div id="totalMarksMeanPieChart"></div>
                                                    </div>
                                                </div>
                                            </fieldset>

                                            <fieldset>
                                                <label style="text-align: center; font-size: 18px;margin-top: 58px; margin-bottom: 20px;" class="col-sm-12">Peer reviews similarity report</label>

                                                <span style="padding-right: 37px; text-align: right;" class="tooltip2 col-sm-12">
                                                    <img style="width: 26px" src="/img/tooltip.png">
                                                    <span class="tooltiptext2">Above 50% is considered a good score</span>
                                                </span>

                                                <div class="col-sm-12" ng-bind-html="rTable"></div>

                                                <div ng-show="!analyzedRewiewData">
                                                    <br>
                                                    <br>
                                                    <label style="text-align: center; font-size: 18px;" class="col-sm-12">No data available</label>
                                                </div>
                                            </fieldset>
                                            <div ng-show="analyzedRewiewData" class="col-sm-12">
                                                <div class="col-sm-6"></div>

                                                <div style="margin-bottom: -54px; " class="col-sm-6">
                                                    <fieldset style="
                                                    text-align:center;
                                                    border: 1px groove threedface;
                                                    padding: 0.5em;
                                                    margin: 4px;
                                                    border-style: groove;
                                                    font-size: 16px;
                                                    font-style: italic;
                                                    width: fit-content;
                                                    float: right;">
                                                        <legend style=" margin-bottom: -6px; width: 71px; border-bottom: 0px; ">Where:</legend>

                                                        <span ng-repeat="legend in legends">
                                                            <b>
                                                                <span style="color: #1f77b4; font-size: 21px;">&diams;</span> {{legend}}&nbsp; &nbsp;</b>
                                                        </span>
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-12">
                                            <div ng-show="analyzedRewiewData" class="col-sm-6">

                                                <label style="text-align: center; font-size: 16px;margin-top: 58px;" class="col-sm-12">Peers reverse reviews</label>

                                                <div class="col-sm-12" id="ReverseReviewsBarChart" style="width: 500px; height: 350px;"></div>
                                            </div>

                                            <div class="col-sm-6">
                                                <br>
                                                <br>
                                                <br>
                                                <div>
                                                    <label style="text-align: center; font-size: 16px;" class="col-sm-12">{{solution.studentName}} reverse reviews</label>
                                                    <br>
                                                    <br>
                                                    <br>
                                                    <div id="ratingPieChart"></div>
                                                </div>

                                            </div>
                                        </div>

                                        <div class="col-sm-12" ng-show="" ng-show="reviews.length == 0">
                                            <label style="text-align: center; font-size: 18px;" class="col-sm-12">No review(s) rating available</label>
                                        </div>

                                    </div>
                                </div>


                                <div class="panel panel-default">
                                    <div class="panel-heading">Review</div>
                                    <div class="panel-body">
                                        <fieldset>
                                            <!-- file upload component -->
                                            <div class="form-group">
                                                <label for="inputReviewDocuments" class="col-sm-4 control-label">Review Documents</label>
                                                <div class="col-sm-2">
                                                    <div class="btn btn-sm btn-default" ngf-select accept="*" ng-model="reviewDocuments" ngf-multiple="true" ngf-keep="true"
                                                        ngf-max-size='1000000'>
                                                        <i class="ionicons ion-document-text"></i> &nbsp; Upload Files
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label"></label>
                                                <div class="col-sm-8">
                                                    <div ng-show="reviewDocuments" ng-repeat="f in reviewDocuments">
                                                        <div class="label label-info">
                                                            &nbsp;
                                                            <span>{{f.name}}</span> &nbsp;
                                                        </div>
                                                        &nbsp;
                                                        <a href="" ng-click="deleteSelectedFiles(f.name)">
                                                            <i class="ion-close-round"></i>
                                                        </a>
                                                        <br>
                                                    </div>
                                                    <div ng-repeat="doc in review.displayDocumentsList">
                                                        <div class="label label-info">
                                                            <a href="{{doc.link}}" target="_blank" style="color: white" download> &nbsp;
                                                                <span>{{doc.name}}</span> &nbsp;</a>
                                                        </div>
                                                        &nbsp;
                                                        <a href="" ng-click="deleteUploadedFiles(doc.link)">
                                                            <i class="ion-close-round"></i>
                                                        </a>
                                                        <br>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- file upload component -->
                                            <div class="form-group">
                                                <label for="inputReviewComments" class="col-sm-4 control-label">Comments</label>
                                                <div class="col-sm-4">
                                                    <textarea rows="3" class="form-control original-form-control" id="inputReviewComments" ng-model="review.comments" placeholder="Comments"
                                                    />
                                                </div>
                                            </div>
                                            <div class="form-group" ng-class="{ 'has-error' : form.marks.$invalid && !form.marks.$pristine }">
                                                <label for="inputObtainedMarks" class="col-sm-4 control-label">Obtained Marks(Total: {{solution.peerReviewId.totalMarks}}) *</label>
                                                <div class="col-sm-2">
                                                    <input type="number" name="marks" min="0" max="{{solution.peerReviewId.totalMarks}}" class="form-control original-form-control"
                                                        id="inputObtainedMarks" ng-model="review.marksObtained" required>
                                                </div>
                                                <p ng-show="form.marks.$invalid && form.marks.$error.min" class="help-block">Marks should be a positive number</p>
                                                <p ng-show="form.marks.$invalid && form.marks.$error.number" class="help-block">Marks should be a number</p>
                                                <p ng-show="form.marks.$invalid && form.marks.$error.max" class="help-block">Marks cannot exceed total marks</p>
                                                <p ng-show="form.marks.$invalid && form.marks.$error.required" class="help-block">Marks are required</p>
                                            </div>
                                        </fieldset>
                                    </div>
                                    <div class="panel-footer" style="height: 55px">
                                        <!--<error-block ng-show="errors.length > 0" messages="errors"></error-block>-->
                                        <!-- Button -->
                                        <div class="controls pull-right">
                                            <button class="btn btn-primary" ng-disabled="!isFormValid()" ng-click="submitReview()"> Save
                                                <spinner show="isLoading"></spinner>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                        </form>
                        </div>
                    </div>
                </div>
            </div>
    </section>
    </div>